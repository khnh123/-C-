#include <iostream>
#include <string>
#include <iterator>
#include <set>

using namespace std;
int main ( ) {
    int n;
    cin >> n; //  Количество операций Q,
    set <string> words;
    for (int i=0; i<n; i++) {
        string a;
        cin >> a;
        words.insert(a);
    }
    cout<<words.size()<<endl;
    return 0;
}
